<%- content_for(:navigation) do %>
  <% =link_to t('pages.navigation.up'), show_path(:pages => parent_path(@current_page_path)) %>
<%  =link_to t('pages.navigation.delete'), destroy_path(:pages => @current_page_path), :method => :delete %>
 <% =link_to t('pages.navigation.edit'), edit_path(:pages => @current_page_path) %>
<%  =link_to t('pages.navigation.add'), add_path(:pages => @current_page_path) %>

<% = title @page.title %>

<% .content= read_html(@page.html_text) %>

<% = render :partial => 'tree' %>